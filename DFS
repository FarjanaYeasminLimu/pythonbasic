   "source": [
    "graphs = {\n",
    " 'A' : ['B','C'],\n",
    " 'B' : ['D', 'E'],\n",
    " 'C' : ['F'],\n",
    " 'D' : [],\n",
    " 'E' : ['F'],\n",
    " 'F' : [],\n",
    "}\n",
    "def dfs(visited, graphs, node,goal):\n",
    "     if node not in visited:\n",
    "             if node==goal:\n",
    "                 print(\"GOAL NODE IS : \",goal)\n",
    "             visited.add(node)\n",
    "             for neighbour in graphs[node]:\n",
    "                      dfs(visited, graphs, neighbour,goal)\n",
    "visited=set()\n",
    "dfs(visited, graphs, 'A','F')\n",
    "\n"
   ]
